#include <iostream>
using namespace std;
int main() {
  int t, n, x;
  cin >> t;
  while (t--) {
    int arr[200001] = {0};
    cin >> n;
    while (n--) {
      cin >> x;
      arr[x]++;
    }
    int maks = 0;
    for (int x : arr) {
      if (x) {
        temp++;
        if (x > maks)
          maks = x;
      }
    }
    if (temp == maks) {
      maks--;
    }
    cout << min(temp, maks) << endl;
  }
}
