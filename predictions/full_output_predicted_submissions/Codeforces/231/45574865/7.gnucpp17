#include <iostream>
#include <math.h>
#include <string.h>
using namespace std;
int main() {
  int n, temp = 0, hasil = 0;
  cin >> n;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < 3; j++) {
      cin >> y;
      if (y == 1) {
        temp++;
      }
    }
    if (temp >= 2) {
      hasil++;
    }
    temp = 0;
  }
  cout << hasil;
  return 0;
}
