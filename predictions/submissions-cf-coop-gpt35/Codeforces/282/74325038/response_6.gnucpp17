#include <iostream>

int main() {
  int n, s = 0;
  std::cin >> n;
  char c;
  for (int i = 0; i < n; i++) {
    std::cin >> c;
    switch (c) {
      case '+':
        ++s;
        break;
      case '-':
        --s;
        break;
      default:
        break;
    }
  }
  std::cout << s;
  return 0;
}