inline void endl() {
    cout << '\n';
}

template<typename T>
inline auto all(T& container) {
    return container.begin(), container.end();
}

template<typename T>
inline auto allr(T& container) {
    return container.rbegin(), container.rend();
}

template<typename T>
inline void RT(T s) {
    cout << s;
    exit(0);
}

template<typename T>
inline void watch(T x) {
    cout << (#x) << " = " << x << '\n';
}

template<typename T>
inline int sz(T& s) {
    return static_cast<int>(s.size());
}