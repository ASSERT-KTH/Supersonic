#include <iostream>
#include <string>
using namespace std;

int main() {
  int a, b, n;
  cin >> a >> b;
  n = (a > b) ? (7 - a) : (7 - b);

  string fraction;
  switch (n) {
    case 1:
      fraction = "1/6";
      break;
    case 2:
      fraction = "1/3";
      break;
    case 3:
      fraction = "1/2";
      break;
    case 4:
      fraction = "2/3";
      break;
    case 5:
      fraction = "5/6";
      break;
    case 6:
      fraction = "1/1";
      break;
  }

  cout << fraction;

  return 0;
}