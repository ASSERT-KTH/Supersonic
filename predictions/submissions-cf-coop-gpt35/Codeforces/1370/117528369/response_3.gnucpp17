#include <iostream>
#include <vector>
#include <algorithm> // for std::max

int main() {
  std::ios_base::sync_with_stdio(false);
  std::cin.tie(NULL);
  std::cout.tie(NULL);
  
  int t;
  std::cin >> t;
  
  while (t--) {
    int n;
    std::cin >> n;
    
    int ans = n / 2; // Optimize loop to calculate the largest i in O(1) time
    
    std::cout << ans << "\n";
  }
  
  return 0;
}