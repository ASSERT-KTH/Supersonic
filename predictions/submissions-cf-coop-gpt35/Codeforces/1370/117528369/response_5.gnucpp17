#include <iostream>
#include <vector>
#include <map>
using namespace std;

int main() {
  int t;
  cin >> t;
  while (t--) {
    int n, ans = 0;
    cin >> n;
    for (int i = n / 2; i > 0; i--) {
      if (i * 2 <= n) {
        ans = i;
        break;
      }
    }
    cout << ans << "\n";
  }
  return 0;
}