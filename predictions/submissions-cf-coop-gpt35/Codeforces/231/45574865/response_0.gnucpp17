#include <cstdio>

int main() {
  int n;
  scanf("%d", &n);
  int hasil = 0;
  for (int i = 0; i < n; i++) {
    int temp = 0;
    int y1, y2, y3;
    scanf("%d %d %d", &y1, &y2, &y3);
    if (y1 == 1) temp++;
    if (y2 == 1) temp++;
    if (y3 == 1) temp++;
    if (temp >= 2) hasil++;
  }
  printf("%d\n", hasil);
  return 0;
}