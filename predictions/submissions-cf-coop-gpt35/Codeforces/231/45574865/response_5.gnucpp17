#include <iostream>

int main() {
  int n, temp = 0, hasil = 0;
  std::cin >> n;
  
  int y1, y2, y3;
  for (int i = 0; i < n; i++) {
    std::cin >> y1 >> y2 >> y3;
    if (y1 == 1) {
      temp++;
    }
    if (y2 == 1) {
      temp++;
    }
    if (y3 == 1) {
      temp++;
    }
    if (temp >= 2) {
      hasil++;
    }
    temp = 0;
  }
  
  std::cout << hasil;
  return 0;
}