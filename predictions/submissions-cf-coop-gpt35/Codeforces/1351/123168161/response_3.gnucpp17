#include <iostream>

int main() {
  int t;
  scanf("%d", &t);
  while (t--) {
    int a1, b1, a2, b2;
    scanf("%d %d %d %d", &a1, &b1, &a2, &b2);
    if (std::max(a1, b1) + std::max(a2, b2) == std::min(a1, b1) + std::min(a2, b2))
      printf("YES\n");
    else
      printf("NO\n");
  }
  return 0;
}