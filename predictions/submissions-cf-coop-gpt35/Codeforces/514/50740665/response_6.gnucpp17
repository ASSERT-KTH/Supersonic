#endif
  int n, m;
  cin >> n >> m;
  for (int i = 0; i < n; i++) {
    string s;
    cin >> s;
    st.insert(get_hash(s));
  }
  for (int i = 0; i < m; i++) {
    string t;
    cin >> t;
    cout << (check(t) ? "YES" : "NO") << endl;
  }
#ifdef DEBUG
  fclose(stdin);
  fclose(stdout);
#endif
  return 0;
}