#include <iostream>
#include <string>
using namespace std;
int main() {
  int n, s = 0;
  cin >> n;
  for (int i = 0; i < n; i++) {
    string str;
    cin >> str;
    if (str[0] == 'X' || str[0] == 'X') {
      s = s + 1;
    }
    if (str == "--X" || str == "X--") {
      s = s - 1;
    }
  }
  cout << s;
  return 0;
}
