#include <iostream>
#include <string>
using namespace std;
int main() {
  int n, s = 0;
  cin >> n;
  for (int i = 0; i < n; i++) {
    string str;
    cin >> str;
    if (str == "++X" || str == "X++") {
      s++;
    }
    if (str == "--X" || str == "X--") {
      --s;
    }
  }
  cout << s;
  return 0;
}
