#include <bits/stdc++.h>
using namespace std;
int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  int t;
  cin >> t;
  while (t--) {
    long long temp, n, m, a = 0, b = 0, c = 0, d = 0;
    cin >> n;
    while (n--) {
      cin >> temp;
      temp % 2 ? a++ : c++;
    }
    cin >> m;
    while (m--) {
      cin >> temp;
      temp % 2 ? b++ : d++;
    }
    cout << a * b + c * d << "\n";
  }
  return 0;
}
