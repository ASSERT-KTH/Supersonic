#include <bits/stdc++.h>
using namespace std;
#define ll long long
const int MOD = 1e9 + 7;
const int N = 2 * 1e5 + 10;
ll dp[11][N];
int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  for (ll i = 0; i <= 9; i++) {
    dp[i][0] = 1;
  }
  for (ll j = 1; j <= 200005; j++) {
    for (ll i = 0; i <= 9; i++) {
      if (i != 9)
        dp[i][j] = (dp[i + 1][j - 1]);
      else
        dp[i][j] = (dp[0][j - 1] + dp[1][j - 1]) % MOD;
    }
  }
  ll t;
  cin >> t;
  while (t--) {
    string s;
    ll m;
    cin >> s >> m;
    ll ans = 0;
    for (ll i = 0; i < s.length(); i++) {
      ans = (ans + dp[s[i] - '0'][m]) % MOD;
    }
    cout << ans << endl;
  }
}