#include <iostream>
using namespace std;

int main() {
  int a, b;
  cin >> a >> b;
  
  if (a > b) {
    if (a == 6 && b == 1)
      cout << "1/6";
    else if (a == 5 && b == 2)
      cout << "1/3";
    else if (a == 4 && b == 3)
      cout << "1/2";
    else if (a == 3 && b == 4)
      cout << "2/3";
    else if (a == 2 && b == 5)
      cout << "5/6";
    else if (a == 1 && b == 6)
      cout << "1/1";
  } else {
    if (b == 6 && a == 1)
      cout << "1/6";
    else if (b == 5 && a == 2)
      cout << "1/3";
    else if (b == 4 && a == 3)
      cout << "1/2";
    else if (b == 3 && a == 4)
      cout << "2/3";
    else if (b == 2 && a == 5)
      cout << "5/6";
    else if (b == 1 && a == 6)
      cout << "1/1";
  }
  
  return 0;
}