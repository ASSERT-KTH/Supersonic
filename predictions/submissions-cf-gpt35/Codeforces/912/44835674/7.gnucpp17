cn ll MAX = 1e18;
void safe_mul(ll &a, int b) {
  if (a <= MAX / b)
    a *= b;
  else
    a = -1;
}
void gen_all(cn vector<int> &src, vector<ll> &dest, ll cur = 1, int ind = 0) {
  if (ind == src.size()) {
    dest.pb(cur);
    return;
  }
  while (cur != -1) {
    gen_all(src, dest, cur, ind + 1);
    safe_mul(cur, src[ind]);
  }
}
vector<ll> A, B;
ll cnt(ll val) {
  int ind = B.size() - 1;
  ll res = 0;
  for (cauto e : A) {
    if (e > val)
      break;
    res += upper_bound(CONT(B), val / e) - begin(B);
  }
  return res;
}
