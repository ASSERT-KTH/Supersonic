#include <bits/stdc++.h>
using namespace std;
int main() {
  int t;
  cin >> t;
  while (t--) {
    long long temp, n, m, a = 0, b = 0, c = 0, d = 0;
    cin >> n;
    for (int i = 0; i < n; i++) {
      cin >> temp;
      temp % 2 ? a++ : c++;
    }
    cin >> m;
    for (int i = 0; i < m; i++) {
      cin >> temp;
      temp % 2 ? b++ : d++;
    }
    cout << a * b + c * d << "\n";
  }
  return 0;
}
