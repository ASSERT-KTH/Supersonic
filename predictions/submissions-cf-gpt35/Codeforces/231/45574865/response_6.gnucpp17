#include <iostream>
using namespace std;

int main() {
  int n, hasil = 0;
  cin >> n;
  
  for (int i = 0; i < n; i++) {
    int temp = 0;
    
    for (int j = 0; j < 3; j++) {
      int y;
      cin >> y;
      
      if (y == 1) {
        temp++;
      }
    }
    
    if (temp >= 2) {
      hasil++;
    }
  }
  
  cout << hasil;
  return 0;
}