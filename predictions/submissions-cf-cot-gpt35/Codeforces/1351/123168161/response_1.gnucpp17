#include <iostream>

int main() {
  int t;
  std::cin >> t;

  while (t--) {
    int a1, b1, a2, b2;
    std::cin >> a1 >> b1 >> a2 >> b2;

    if (std::max(a1, b1) == std::max(a2, b2) && (a1 + b1) == std::max(a1, b1))
      std::cout << "YES\n";
    else
      std::cout << "NO\n";
  }

  return 0;
}