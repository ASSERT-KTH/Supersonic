#include <iostream>
using namespace std;
int main() {
  int t;
  scanf("%d", &t);
  while (t--) {
    int a1, b1, a2, b2;
    scanf("%d %d %d %d", &a1, &b1, &a2, &b2);
    int max1 = (a1 > b1) ? a1 : b1;
    int max2 = (a2 > b2) ? a2 : b2;
    if (a1 + b1 == max1 && a2 + b2 == max1)
      printf("YES\n");
    else
      printf("NO\n");
  }
  return 0;
}