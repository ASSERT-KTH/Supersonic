#include <iostream>

int main() {
    int t;
    std::cin >> t;
    for (int i = 0; i < t; ++i) {
        int a1, b1, a2, b2;
        std::cin >> a1 >> b1 >> a2 >> b2;
        if (a1 + b2 == a2 + b1)
            std::cout << "Possible" << std::endl;
        else
            std::cout << "Impossible" << std::endl;
    }
    return 0;
}