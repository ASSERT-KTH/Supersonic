#include <iostream>
#include <cmath>
using namespace std;

int digits(int n) {
  return floor(log10(n) + 1);
}

void solve() {
  int a, b, c;
  cin >> a >> b >> c;
  
  int x = pow(10, c - 1);
  int i = pow(10, a - 1);
  int j = pow(10, b - 1);
  
  cout << i << " " << j << endl;
}

int main() {
  int t;
  cin >> t;
  
  while (t--) {
    solve();
  }
  
  return 0;
}