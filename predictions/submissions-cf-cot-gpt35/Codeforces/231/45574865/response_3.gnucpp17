#include <iostream>

int main() {
  int n;
  std::cin >> n;

  int hasil = 0;
  for (int i = 0; i < n; i++) {
    int temp = 0;
    for (int j = 0; j < 3; j++) {
      int input;
      std::cin >> input;
      if (input == 1) {
        temp++;
      }
    }
    if (temp >= 2) {
      hasil++;
    }
  }

  std::cout << hasil;
  return 0;
}