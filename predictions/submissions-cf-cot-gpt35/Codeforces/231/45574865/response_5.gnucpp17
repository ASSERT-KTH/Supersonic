#include <iostream>
using namespace std;

int main() {
  int n, hasil = 0;
  cin >> n;

  int y;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < 3; j++) {
      cin >> y;
      if (y == 1) {
        hasil++;
        if (hasil >= 2) {
          break;
        }
      }
    }
  }

  cout << hasil;
  return 0;
}