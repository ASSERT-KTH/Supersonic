#include <algorithm>
#include <iostream>
#include <vector>
#include <cstdlib>

int main() {
  int n, m;
  std::cin >> n >> m;
  
  std::vector<int> arr(n);
  for (int i = 0; i < n; i++) {
    std::cin >> arr[i];
  }
  
  std::partial_sort(arr.begin(), arr.begin() + m, arr.end());
  
  int sum = 0;
  for (int i = 0; i < m; i++) {
    if (arr[i] < 0)
      sum += arr[i];
    else
      break;
  }
  
  std::cout << std::abs(sum) << std::endl;
  return 0;
}