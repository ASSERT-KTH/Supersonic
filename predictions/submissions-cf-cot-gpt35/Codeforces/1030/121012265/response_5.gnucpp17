#include <iostream>

int main() {
  std::cin.tie(NULL);
  int n, a;
  std::cin >> n;
  bool isHard = false;
  for (int i = 0; i < n; i++) {
    std::cin >> a;
    if (a == 1) {
      isHard = true;
      break;
    }
  }
  if (isHard) {
    std::cout << "HARD" << std::endl;
  } else {
    std::cout << "EASY" << std::endl;
  }
  return 0;
}