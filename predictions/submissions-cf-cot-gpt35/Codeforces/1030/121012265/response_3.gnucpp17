#include <iostream>
using namespace std;

int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);

  int n;
  cin >> n;

  bool hard = false;
  for (int i = 0; i < n; i++) {
    int a;
    cin >> a;
    if (a == 1) {
      hard = true;
    }
  }

  if (hard) {
    cout << "HARD\n";
  } else {
    cout << "EASY\n";
  }

  return 0;
}