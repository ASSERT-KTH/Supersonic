#include <iostream>

int main() {
  std::ios_base::sync_with_stdio(false);

  int n, a;
  std::cin >> n;

  bool hard = false;
  while (n--) {
    std::cin >> a;
    if (a == 1) {
      hard = true;
      break;
    }
  }

  if (hard) {
    std::cout << "HARD\n";
  } else {
    std::cout << "EASY\n";
  }

  return 0;
}