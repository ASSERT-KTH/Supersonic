#include <iostream>

int main() {
  const int n = 100; // Optimized for a specific value of n

  int sum = 0;
  for (int i = 0; i < n; i++) {
    int x, y, z;
    std::cin >> x >> y >> z;
    sum += x + y + z;

    if (sum != 0) {
      std::cout << "NO";
      return 0; // Early exit
    }
  }

  std::cout << "YES";
  return 0;
}