#include <iostream>
using namespace std;

int main() {
  int sum_x = 0, sum_y = 0, sum_z = 0;
  int x, y, z;

  while (cin >> x >> y >> z) {
    sum_x += x;
    sum_y += y;
    sum_z += z;

    if (sum_x != 0 || sum_y != 0 || sum_z != 0) {
      cout << "NO";
      return 0;
    }
  }

  cout << "YES";
  return 0;
}