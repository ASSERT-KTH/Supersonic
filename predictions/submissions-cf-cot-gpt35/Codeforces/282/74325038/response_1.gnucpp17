#include <iostream>
using namespace std;

int main() {
  int n, s = 0;
  cin >> n;
  
  for (int i = 0; i < n; i++) {
    char op1, op2;
    cin >> op1 >> op2;
    
    if (op1 == '+' && op2 == '+') {
      s = s + 1;
    }
    if (op1 == '-' && op2 == '-') {
      s = s - 1;
    }
  }
  
  cout << s;
  return 0;
}