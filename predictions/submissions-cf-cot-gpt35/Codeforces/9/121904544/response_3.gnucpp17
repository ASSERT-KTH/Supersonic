#include <iostream>

int main() {
  int a, b, n;
  
  // Read input values
  std::cin >> a >> b;

  // Calculate n
  n = (a > b) ? 7 - a : 7 - b;

  // Array of possible outputs
  std::string outputs[7] = { "", "1/6", "1/3", "1/2", "2/3", "5/6", "1/1" };

  // Print the output
  std::cout << outputs[n];

  return 0;
}