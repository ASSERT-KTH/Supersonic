#include <iostream>
using namespace std;
int main() {
  int n, a, b, c;
  cin >> n >> a >> b >> c;
  int ans = 0;
  for (int A = 0; A*a <= n; A++) {
    for (int B = 0; B*b + A*a <= n; B++) {
      int C = (n - (A * a + B * b)) / c;
      if (C >= 0 && (n - (A * a + B * b)) % c == 0) {
        ans = max(ans, A + B + C);
      }
    }
  }
  cout << ans << endl;
  return 0;
}