#include <bits/stdc++.h>
#define x first
#define y second
#define pb push_back
#define eb emplace_back
#define all(a) (a).begin(), (a).end()
#define SZ(a) (int)(a).size()
#define FOR(i, a, b) for (int i = (a); i <= (b); ++i)
#define ROF(i, a, b) for (int i = (a); i >= (b); --i)
#define make_unique(a) (a).erase(unique(all((a))), (a).end())
using namespace std;
typedef pair<int, int> PII;
typedef long long LL;
typedef double DD;
typedef long double LD;
typedef pair<LL, LL> PLL;
typedef pair<DD, DD> PDD;
typedef vector<int> VI;
typedef vector<LL> VL;
const int N = 62;
const int mod = 998244353;
int dp[N][N][N], way[10][N][N];
unordered_set<string> mp;
char s[10];
int dict(char a) {
  if (isalpha(a)) {
    if (isupper(a))
      return a - 'A';
    return a - 'a' + 26;
  }
  return a - '0' + 52;
}
void adding(int &a, int b) {
  a = (a += b) >= mod ? a - mod : a;
}
int add(int a, int b) { return ((a += b) >= mod) ? a - mod : a; }
int mul(int a, int b) { return 1LL * a * b % mod; }
int play(int i, int j, int k) {
  int mx = max(max(i, j), k);
  int mi = min(min(i, j), k);
  return dp[mx][i ^ j ^ k ^ mx ^ mi][mi];
}
void solve() {
  int n;
  scanf("%d", &n);
  FOR(i, 1, n) {
    scanf(" %s", s);
    int len = strlen(s) - 1;
    if (mp.find(s) == mp.end())
      way[len][dict(s[0])][dict(s[len))]++, mp.insert(s);
    reverse(s, s + len + 1);
    if (mp.find(s) == mp.end())
      way[len][dict(s[0])][dict(s[len])]++, mp.insert(s);
  }
  int ans = 0;
  FOR(L, 2, 9) {
    FOR(i, 0, 61) {
      FOR(j, 0, i) {
        FOR(k, 0, j) {
          dp[i][j][k] = 0;
          FOR(l, 0, 61) {
            int a = mul(mul(way[L][i][l], way[L][j][l]), way[L][k][l]);
            adding(dp[i][j][k], a);
          }
        }
      }
    }
    FOR(i, 0, 61) {
      FOR(j, 0, i) {
        FOR(k, 0, 61) {
          FOR(l, 0, k) {
            int a = mul(mul(play(i, k, l), play(j, l, k)),
                        mul(play(i, j, k), play(i, l, j)));
            if (k != l)
              adding(a, a);
            if (i != j)
              adding(a, a);
            adding(ans, a);
          }
        }
      }
    }
  }
  printf("%d\n", ans);
}
int main() {
  solve();
  return 0;
}