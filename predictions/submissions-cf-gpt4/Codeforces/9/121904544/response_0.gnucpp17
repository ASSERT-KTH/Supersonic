#include <iostream>
using namespace std;
int main() {
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  int a, b;
  cin >> a >> b;
  switch (7 - max(a, b)) {
  case 1:
    cout << "1/6";
    break;
  case 2:
    cout << "1/3";
    break;
  case 3:
    cout << "1/2";
    break;
  case 4:
    cout << "2/3";
    break;
  case 5:
    cout << "5/6";
    break;
  case 6:
    cout << "1/1";
    break;
  default:
    break;
  }
  return 0;
}