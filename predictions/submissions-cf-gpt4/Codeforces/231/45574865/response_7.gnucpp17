#include <iostream>
using namespace std;
int main() {
  ios_base::sync_with_stdio(false); 
  cin.tie(NULL);
  int n, temp = 0, hasil = 0;
  cin >> n;
  int y;
  while (n--) {
    temp = 0;
    for (int j = 0; j < 3; j++) {
      cin >> y;
      temp += y;
    }
    hasil += (temp >= 2);
  }
  cout << hasil;
  return 0;
}