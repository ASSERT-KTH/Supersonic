#include <iostream>
using namespace std;
int main() {
  ios::sync_with_stdio(0);
  cin.tie(0);
  int n, y, hasil = 0;
  cin >> n;
  for (int i = 0; i < n; i++) {
    int temp = 0;
    for (int j = 0; j < 3; j++) {
      cin >> y;
      temp += y;
    }
    if (temp >= 2) {
      hasil++;
    }
  }
  cout << hasil;
  return 0;
}