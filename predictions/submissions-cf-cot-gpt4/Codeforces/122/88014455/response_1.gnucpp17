#include <iostream>
using namespace std;
#define ll long long

const ll divisors[] = {4, 7, 44, 47, 74, 77, 444, 447, 474, 477, 744, 747, 774, 777};

void solve() {
  ll n;
  cin >> n;
  for(int i = 0; i < 14; i++) {
    if (n % divisors[i] == 0) {
      cout << "YES\n";
      return;
    }
  }
  cout << "NO\n";
}

int main() {
#ifdef APAN
  freopen("input.txt", "r", stdin);
  freopen("output.txt", "w", stdout);
#endif
  solve();
  return 0;
}