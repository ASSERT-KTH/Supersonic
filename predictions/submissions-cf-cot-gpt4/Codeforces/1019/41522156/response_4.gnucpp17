#include <iostream>
using namespace std;
typedef long long int64;
int64 x[2180], y[2180];
int64 Nx[2180], Ny[2180];
int main() {
  int n;
  int64 S;
  cin >> n >> S;
  S *= 2L;
  for (int i = 0; i < n; ++i) {
    cin >> x[i] >> y[i];
  }
  for (int s = 0; s < n; ++s) {
    for (int i = s + 1; i < n; ++i) {
      Nx[i] = x[i] - x[s];
      Ny[i] = y[i] - y[s];
    }
    for (int i = s + 1; i < n; ++i) {
      int64 Xi = Nx[i], Yi = Ny[i];
      for (int j = i + 1; j < n; ++j) {
        int64 t = Nx[j] * Yi - Ny[j] * Xi;
        if (t == S || t == -S) {
          cout << "Yes\n";
          cout << x[s] << " " << y[s] << "\n";
          cout << x[i] << " " << y[i] << "\n";
          cout << x[j] << " " << y[j] << "\n";
          return 0;
        }
      }
    }
  }
  cout << "No\n";
  return 0;
}