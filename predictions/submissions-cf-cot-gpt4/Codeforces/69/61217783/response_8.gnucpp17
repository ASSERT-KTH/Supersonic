#include <cstdio>

int main() {
    int n, x, y, z;
    int sum_x = 0, sum_y = 0, sum_z = 0;

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%d %d %d", &x, &y, &z);
        sum_x += x;
        sum_y += y;
        sum_z += z;
    }

    printf(sum_x == 0 && sum_y == 0 && sum_z == 0 ? "YES\n" : "NO\n");
    
    return 0;
}