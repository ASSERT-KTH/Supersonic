#include <iostream>
using namespace std;

int main() {
  ios::sync_with_stdio(false);
  cin.tie(NULL);

  int n;
  cin >> n;

  int sum_x = 0, sum_y = 0, sum_z = 0;
  
  for (int i = 0; i < n; i++) {
    int x, y, z;
    cin >> x >> y >> z;
    sum_x += x;
    sum_y += y;
    sum_z += z;
    
    if (sum_x != 0 || sum_y != 0 || sum_z != 0) {
      cout << "NO" << '\n';
      return 0;
    }
  }
  
  cout << "YES" << '\n';
  return 0;
}