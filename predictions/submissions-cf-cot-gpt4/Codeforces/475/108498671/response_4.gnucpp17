#include <iostream>
#include <vector>
using namespace std;

int gcd(int a, int b) {
  if (b == 0) {
    return a;
  }
  return gcd(b, a % b);
}

int get_log[200100];
int st[100100][20];
long long ans[100100];

int get(int l, int r) {
  int sz = get_log[r - l];
  return gcd(st[l][sz], st[r - (1LL << sz)][sz]);
}

int main() {
  ios_base::sync_with_stdio(false); cin.tie(NULL);
  int n;
  
  get_log[1] = 0;
  for (int i = 2; i < 200000; i++) {
    get_log[i] = get_log[i - 1];
    while ((1 << get_log[i]) <= i)
      get_log[i]++;
    get_log[i]--;
  }
  
  cin >> n;
  vector<int> a(n);
  for (int i = 0; i < n; i++) {
    cin >> a[i];
    st[i][0] = a[i];
  }
  
  for (int nowP = 1; nowP < 20; nowP++) {
    for (int i = 0; i + (1LL << nowP) <= n; i++) {
      st[i][nowP] = gcd(st[i][nowP - 1], st[i + (1LL << (nowP - 1))][nowP - 1]);
    }
  }
  
  for (int L = 0; L < n; L++) {
    int prev_ind = L;
    int now_ind = L;
    while (now_ind < n) {
      int now_gcd = get(L, now_ind + 1);
      int l = now_ind;
      int r = n;
      while (r - l > 1) {
        int m = (l + r) / 2;
        int x = get(L, m + 1);
        if (x < now_gcd)
          r = m;
        else
          l = m;
      }
      ans[now_gcd] += r - prev_ind;
      now_ind = r;
      prev_ind = r;
    }
  }
  
  int q;
  cin >> q;
  while (q--) {
    int x;
    cin >> x;
    cout << ans[x] << '\n';
  }
  
  return 0;
}