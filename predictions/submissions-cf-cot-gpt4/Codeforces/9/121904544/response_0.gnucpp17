#include <iostream>

int main() {
  int a;
  int b;
  std::cin >> a >> b;
  
  switch (7 - std::max(a, b)) {
  case 1:
    std::cout << "1/6";
    break;
  case 2:
    std::cout << "1/3";
    break;
  case 3:
    std::cout << "1/2";
    break;
  case 4:
    std::cout << "2/3";
    break;
  case 5:
    std::cout << "5/6";
    break;
  case 6:
    std::cout << "1/1";
    break;
  default:
    std::cout << "Invalid input";
    break;
  }
  return 0;
}