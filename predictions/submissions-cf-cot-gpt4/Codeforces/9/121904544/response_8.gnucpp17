#include <cstdio>
#include <algorithm>
using namespace std;
int main() {
  int a, b;
  scanf("%d %d", &a, &b);
  switch (7 - max(a, b)) {
  case 1:
    printf("1/6");
    break;
  case 2:
    printf("1/3");
    break;
  case 3:
    printf("1/2");
    break;
  case 4:
    printf("2/3");
    break;
  case 5:
    printf("5/6");
    break;
  case 6:
    printf("1/1");
    break;
  }
}