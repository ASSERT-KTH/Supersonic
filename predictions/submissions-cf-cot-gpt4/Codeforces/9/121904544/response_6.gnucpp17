#include <iostream>
using namespace std;

int main() {
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(NULL);
    std::cout.tie(NULL);

    int a, b;
    cin >> a >> b;

    switch (7 - max(a, b)) {
        case 1:
            cout << "1/6\n";
            break;
        case 2:
            cout << "1/3\n";
            break;
        case 3:
            cout << "1/2\n";
            break;
        case 4:
            cout << "2/3\n";
            break;
        case 5:
            cout << "5/6\n";
            break;
        case 6:
            cout << "1/1\n";
            break;
    }

    return 0;
}