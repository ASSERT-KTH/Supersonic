#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define int long long
#define w(t) int t; cin >> t; while (t--)

signed main() {
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  
  w(t) {
    int n;
    cin >> n;
    int ans = n / 2;
    cout << ans << "\n";
  }
  
  return 0;
}