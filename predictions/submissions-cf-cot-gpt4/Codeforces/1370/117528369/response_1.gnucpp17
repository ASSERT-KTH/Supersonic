#include <bits/stdc++.h>
using namespace std;
#define w(t) int t; cin >> t; while (t--)
signed main() {
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  cout.tie(NULL);
  w(t) {
    int n;
    cin >> n;
    for (int i = n / 2; i > 0; i--) {
      int num = i * 2;
      if (num <= n) {
        cout << i << "\n";
        break;
      }
    }
  }
  return 0;
}