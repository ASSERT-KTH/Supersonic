#include <iostream>
using namespace std;

int main() {
    int n, s = 0;
    cin >> n;
    for (int i = 0; i < n; i++) {
        char operation;
        cin >> operation;
        if (operation == '+') {
            s++;
        } else if (operation == '-') {
            s--;
        }
    }
    cout << s;
    return 0;
}