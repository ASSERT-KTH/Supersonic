#include <iostream>
#include <string>
using namespace std;

int main() {
  int n, s = 0;
  cin >> n;
  string str;
  for (int i = 0; i < n; i++) {
    cin >> str;
    switch(str[1]) {
      case '+':
        ++s;
        break;
      case '-':
        --s;
        break;
    }
  }
  cout << s;
  return 0;
}